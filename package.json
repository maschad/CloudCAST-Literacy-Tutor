{
  "name": "uwi-cloudcast-platform",
  "version": "0.8.0",
  "description": "Literacy tutor built with Angular2, AngularFire2, and Firebase",
  "homepage": "https://github.com/r-park/todo-angular2-firebase",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.8"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "clean:target": "del-cli ./target",
    "lint": "run-s lint:js lint:ts",
    "lint:js": "eslint -c .eslintrc.yml *.js ./server",
    "lint:ts": "tslint './src/**/*.ts'",
    "prebuild": "run-s clean:target test",
    "pretest": "run-s lint",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' ./server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start --reporters dots"
  },
  "dependencies": {
    "@angular/animations": "^4.2.5",
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/material": "^2.0.0-beta.7",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "@angular/upgrade": "2.2.0",
    "angular-in-memory-web-api": "~0.3.0",
    "angular2-elastic": "^0.13.0",
    "angularfire2": "2.0.0-beta.5",
    "chart.js": "^2.5.0",
    "core-js": "^2.4.1",
    "firebase": "3.5.2",
    "hammerjs": "^2.0.8",
    "ng-sidebar": "^4.2.3",
    "ng2-charts": "^1.5.0",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/core-js": "0.9.35",
    "@types/jasmine": "~2.5.36",
    "@types/node": "~6.0.46",
    "@types/request": "0.0.30",
    "autoprefixer": "~6.5.1",
    "copy-webpack-plugin": "~4.0.1",
    "cross-env": "~3.1.3",
    "css-loader": "~0.25.0",
    "del-cli": "~0.2.0",
    "eslint": "~3.10.2",
    "express": "~4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "firebase-tools": "3.1.0",
    "html-webpack-plugin": "~2.24.1",
    "jasmine-core": "~2.5.2",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-jasmine": "~1.0.2",
    "karma-mocha-reporter": "~2.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.8.0",
    "minx": "r-park/minx.git",
    "morgan": "~1.7.0",
    "node-sass": "~3.13.0",
    "nodemon": "~1.11.0",
    "npm-run-all": "~3.1.1",
    "postcss-loader": "~1.1.0",
    "raw-loader": "~0.5.1",
    "sass-loader": "~4.0.2",
    "style-loader": "~0.13.1",
    "ts-helpers": "~1.1.2",
    "ts-loader": "~0.9.5",
    "tslint": "~3.15.1",
    "typescript": "~2.2.1",
    "typings": "~2.0.0",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "2.1.0-beta.4",
    "webpack-md5-hash": "~0.0.5",
    "winston": "~2.3.0"
  }
}
